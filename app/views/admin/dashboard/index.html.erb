<h1>Admin Dashboard</h1>

<ul>
  <li><%= link_to 'Admin Merchants', admin_merchants_path %></li>
  <li><%= link_to 'Admin Invoices', admin_invoices_path %></li>
</ul>



<div id="top-5">
  <%= react_component("Top5AdminDashboard", { top5: @top5 }) %>
</div>

<div id="incomplete_invoices">
  <h1>Incomplete Invoices</h1>
  <ul>
    <% @incomp_invoices.each do |i| %>
      <%= react_component("InCompleteInvoices", { invoice_id: i.id, date: i.created_at.httpdate }) %>
    <% end %>
  </ul>

</div>